{
    "name": "adamo-nest-server",
    "version": "0.0.1",
    "description": "Awesome nestjs boilerplate wrote in Typescript",
    "author": "Narek Hakobyan <narek.hakobyan.07@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "compodoc": "npx compodoc -p tsconfig.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:hmr": "node dist/main",
        "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
        "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
        "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
        "watch:dev": "nodemon --config nodemon.json",
        "debug:dev": "nodemon --config nodemon-debug.json",
        "webpack": "webpack --config webpack.config.js --progress",
        "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
        "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
        "schema:sync": "ts-node node_modules/typeorm/cli.js schema:sync -f ormconfig",
        "start:prod": "node dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "lint:fix": "tslint --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "docs:dev": "vuepress dev -p 7070",
        "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
        "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist",
        "seed-old": "tsc && node src/database/seeders/database.seeder.js",
        "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
    },
    "dependencies": {
        "@nestjs/common": "^6.11.11",
        "@nestjs/core": "^6.11.11",
        "@nestjs/jwt": "^6.0.0",
        "@nestjs/microservices": "^6.0.1",
        "@nestjs/passport": "^6.0.0",
        "@nestjs/platform-express": "^6.11.11",
        "@nestjs/platform-socket.io": "^6.11.11",
        "@nestjs/swagger": "^3.0.1",
        "@nestjs/typeorm": "^6.3.3",
        "@nestjs/websockets": "^6.11.11",
        "aws-sdk": "^2.405.0",
        "axios": "^0.21.1",
        "bcrypt": "^3.0.4",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.9.1",
        "compression": "^1.7.3",
        "cookie-parser": "^1.4.4",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "express-rate-limit": "~3.3.0",
        "express-session": "^1.17.0",
        "fast-xml-parser": "^3.16.0",
        "file-type": "^10.8.0",
        "helmet": "^3.16.0",
        "jsonwebtoken": "^8.4.0",
        "lodash": "^4.17.11",
        "mime-types": "^2.1.22",
        "mongodb": "^3.1.13",
        "morgan": "^1.9.1",
        "nest-schedule": "^0.4.5",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^7.18.2",
        "raw-loader": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "request-context": "^2.0.0",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "showdown": "^1.9.1",
        "string-similarity": "^4.0.1",
        "swagger-ui-express": "^4.0.2",
        "typeorm": "^0.2.24",
        "typeorm-seeding": "^1.0.0-rc.3",
        "typeorm-transactional-cls-hooked": "^0.1.7",
        "uuid": "^3.3.2",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.10",
        "@nestjs/cli": "^6.14.2",
        "@nestjs/schematics": "^6.9.4",
        "@nestjs/testing": "^6.11.11",
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "^0.0.36",
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.17.3",
        "@types/express-rate-limit": "~3.3.0",
        "@types/express-session": "^1.15.16",
        "@types/file-type": "^10.6.0",
        "@types/helmet": "^0.0.43",
        "@types/jest": "^25.1.4",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/lodash": "^4.14.126",
        "@types/mime-types": "^2.1.0",
        "@types/morgan": "^1.7.35",
        "@types/node": "^13.9.0",
        "@types/passport-jwt": "^3.0.1",
        "@types/socket.io": "^2.1.4",
        "@types/socket.io-client": "^1.4.32",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^3.4.4",
        "clean-webpack-plugin": "^2.0.1",
        "gh-pages": "^2.0.1",
        "husky": "^4.2.3",
        "jest": "^25.1.0",
        "lint-staged": "^10.0.8",
        "nodemon": "^1.18.3",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.1.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "tslint": "5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-consistent-codestyle": "^1.15.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "^3.8.3",
        "vuepress": "^0.14.10",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-node-externals": "^1.7.2"
    },
    "jest": {
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
